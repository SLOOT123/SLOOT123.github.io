{
  "mcpServers": {
    "supabase": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--read-only",
        "--project-ref",
        "YOUR_PROJECT_REF"
      ],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "env:SERVICE_SUPABASE_ACCESS_TOKEN"
      },
      "disabled": false
    },
    "stripe": {
      "command": "npx",
      "args": [
        "-y",
        "@stripe/mcp",
        "--tools=all"
      ],
      "env": {
        "STRIPE_SECRET_KEY": "env:STRIPE_SECRET_KEY"
      },
      "disabled": false
    },
    "Context7": {
      "command": "/users/mverab/.nvm/versions/node/v22.8.0/bin/npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "disabled": false
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "env": {},
      "disabled": false
    },
    "fetch": {
      "command": "node",
      "args": [
        "/absolute/path/to/fetch-mcp-server.js"
      ],
      "env": {},
      "disabled": false
    },
    "agent-zero": {
      "type": "sse",
      "url": "http://localhost:32768/mcp/t-mWDlt3BFNePBzz9R/sse",
      "disabled": false
    }
  }
}